   "metadata": {},
   "outputs": [],
   "source": [
    "######03/08/2018 code to identify sites in ROMS model vertices list from Liz Drenkard\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
